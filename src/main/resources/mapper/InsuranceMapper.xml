<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.bunsanedthinking_springback.repository.InsuranceMapper">
    <select id="findById_FinancialAccountant" resultType="com.example.bunsanedthinking_springback.vo.InsuranceVO">
        select *
        from insurance
        where product_id=#{param1}
    </select>
    <select id="findByInsuranceType_Compensation" resultType="com.example.bunsanedthinking_springback.vo.InsuranceVO">
        select *
        from insurance
        where insurance_type=#{param1}
    </select>
</mapper>
