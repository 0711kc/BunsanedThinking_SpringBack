<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.bunsanedthinking_springback.repository.ContractMapper">
    <select id="findById_FinancialAccountant" resultType="com.example.bunsanedthinking_springback.vo.ContractVO">
        select *
        from contract
        where id=#{param1}
    </select>
    <select id="findByCustomerIdAndProductId_FinancialAccountant" resultType="com.example.bunsanedthinking_springback.vo.ContractVO">
        select *
        from contract
        where customer_id=#{customer_id}
        and product_id=#{product_id}
    </select>
</mapper>
