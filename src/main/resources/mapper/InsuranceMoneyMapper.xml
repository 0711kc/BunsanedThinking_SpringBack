<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.bunsanedthinking_springback.repository.InsuranceMoneyMapper">
    <select id="getAllByContractId_Customer" resultType="com.example.bunsanedthinking_springback.vo.InsuranceMoneyVO">
        select * from insurancemoney
        where contract_id = #{id}
    </select>
    <select id="getAll_Compensation" resultType="com.example.bunsanedthinking_springback.vo.InsuranceMoneyVO">
        select * from insurancemoney
    </select>
    <select id="getById_Compensation" resultType="com.example.bunsanedthinking_springback.vo.InsuranceMoneyVO">
        select * from insurancemoney
        where id = #{id}
    </select>
    <select id="getAll_UnderwritingModel" resultType="com.example.bunsanedthinking_springback.vo.InsuranceMoneyVO">
        select * from InsuranceMoney
    </select>
    <select id="findByContractId_FinancialAccountant" resultType="com.example.bunsanedthinking_springback.vo.InsuranceMoneyVO">
        select *
        from insurancemoney
        where contract_id=#{param1}
    </select>
    <update id="updateStatus_Compensation">
        update insurancemoney
        set process_status = #{status}
        where id = #{insuranceMoneyId}
    </update>
</mapper>
