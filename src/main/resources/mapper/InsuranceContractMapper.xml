<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.bunsanedthinking_springback.repository.InsuranceContractMapper">
    <insert id="insert_LoanManagement" parameterType="map">
        insert into insurancecontract(product_id, insurance_id)
        values (
                #{insuranceContract.product_id},
                #{insuranceContract.insurance_id}
               )
    </insert>

    <select id="getAllInsuranceContractLoan_SalesModel" resultType="com.example.bunsanedthinking_springback.vo.InsuranceContractVO">
        select * from insurancecontract
    </select>
</mapper>
