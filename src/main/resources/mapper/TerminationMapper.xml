<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.bunsanedthinking_springback.repository.TerminationMapper">
    <select id="getById_ContractManagement" resultType="com.example.bunsanedthinking_springback.vo.TerminationVO">
        select * from termination
        where contract_id = #{id}
    </select>
    <select id="getAll_ContractManagement" resultType="com.example.bunsanedthinking_springback.vo.TerminationVO">
        select * from termination
    </select>
</mapper>